{
    "builds": [
      { "src": "vercel_build.sh", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/vercel_build.sh" }
    ]
  ,"routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "myweb/wsgi.py"
    }
  ]
}
  